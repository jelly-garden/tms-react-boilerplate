{
    "typescript.tsdk": "node_modules/typescript/lib",
    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/dist": true,
        "**/bin": true,
        "**/logs": true,
        "**/assets": true,
        "**/index.d.ts": true,
        "yarn.lock": true,
        "package-lock.json": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "node_modules": false
    },
    "colorize.languages": [
        "javascript",
        "typescript"
    ],
    "editor.rulers": [120],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "eslint.alwaysShowStatus": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "cSpell.words": [
        "Appstore",
        "auths",
        "exapp",
        "Formik",
        "Gids",
        "inapp",
        "isnew",
        "Noto",
        "persistor",
        "reduxjs",
        "Seongjin",
        "toastify",
        "Uids"
    ]
}
